
@{
    ViewBag.Title = "Index";
    Layout = Request.IsAjaxRequest() ? null : "~/Views/Shared/_Layout_Adminlte.cshtml";
}


<section class="content-header">
    <h1>
        @T("View Students' Mock Test Report")
        <sup class="text-blue">@T("Creation")</sup>
    </h1>
    <ol class="breadcrumb">
        <li><a href="#"><i class="fa fa-dashboard"></i> @T("Home")</a></li>
        <li class="active">@T("Setup")</li>
    </ol>
</section>
<!-- Main Content-->
<section class="content">
    <div class="row">
        <div class="col-lg-12">
            <div class="box box-primary">

                <div class="box-header">
                    <div class="btn-group">

                        <button type="button" class="btn btn-default " data-bind="click:ViewReport"><i class="fa fa-eye text-blue">&nbsp;@T("View")</i></button>

                    </div>
                </div>
                <div class="box-body">
                    <div class="row">
                        <div class="col-lg-12" data-bind="kendoGrid:KendoGridOptions">
                            <div id="MockTestReportKendoGrid">

                            </div>
                        </div>
                    </div>

                </div>
            </div>
        </div>
    </div>


</section>


@if (Request.IsAjaxRequest())
{
   
    <script type="text/javascript">
        Riddha.global.permission.action('73').done(function (data) {
            Riddha.global.permission.actions = data.Data;
            ko.cleanNode(document.getElementById("partial-render"));
            var obj = new viewStudentsMockTestReportController();
            ko.applyBindings(obj, document.getElementById("partial-render"));
        });
    </script>
}
else
{
    @section scripts
    {
        <script type="text/javascript">
            Riddha.global.permission.action('73').done(function (data) {
                Riddha.global.permission.actions = data.Data;
                ko.cleanNode(document.getElementById("partial-render"));
                var obj = new viewStudentsMockTestReportController();
                ko.applyBindings(obj, document.getElementById("partial-render"));
            });
        </script>
    }
}
